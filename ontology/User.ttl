:intro
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "Introduction"@en ;
  rdfs:domain :User ;
  rdfs:range xsd:string .

:email
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "Email"@en ;
  rdfs:comment "The email address of the User."@en ;
  rdfs:domain :User ;
  rdfs:range xsd:string ;
  rdfs:seeAlso schema:email .

:pictureUrl
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain :User ;
  rdfs:range xsd:string .

:milestoneInstances
  a owl:ObjectProperty ;
  rdfs:domain :User ;
  rdfs:range :MilestoneInstance ;
  owl:inverseOf :sourceUser .

:skillInstances
  rdfs:comment "The Skills Instances the User has."@en ;
  a owl:ObjectProperty ;
  rdfs:domain :User ;
  rdfs:range :SkillInstance ;
  owl:inverseOf :sourceUser .

:commits
  rdfs:comment "The Commits the User has."@en ;
  a owl:ObjectProperty ;
  rdfs:domain :User ;
  rdfs:range :Commit ;
  owl:inverseOf :sourceUser .
